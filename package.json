        "tree-kill": "^1.2.2",
        "vscode-uri": "^3.0.8"
    },
    "devDependencies": {
        "@types/cli-progress": "^3.11.5",
        "@types/follow-redirects": "^1.14.4",
        "@types/node": "20.x",
        "@types/prompts": "^2.4.9",
